    <script>
        $(document).ready(function() {
        	
        	console.log(docArr);
        	
            $("#q").val(q);
            $("#formQ").val(q);
            $("#formField").val(searchField);
            $("#formSort").val(sortType);
            $("#formDateType").val(dateType);
            $("#formStartDate").val(startDate);
            $("#formEndDate").val(endDate);
