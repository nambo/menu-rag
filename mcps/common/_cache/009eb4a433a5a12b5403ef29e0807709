                }
                if(key%3 == 2 || key == (channelParam.length-1)){
                    channelParamHtml +="</tr>"
                }
            })
            channelParamHtml += '</table>'
            $("#channelParam").html(channelParamHtml);
            //新闻列表
            var docListHtml = '';
            var re1 = /<em>/gi;
            var re2 = /<\/em>/gi;
            
