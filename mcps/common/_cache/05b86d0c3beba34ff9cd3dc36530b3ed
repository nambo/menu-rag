            //页码
            if (docArr.length > 0) {
               
            	var pagestr=getPages(totalNum,pageSize,pageNum);
            	var pageDIV = $("#pageDiv");
                pageDIV.append(pagestr);
            }

            if(shouQiFlag == 'hide'){
                hideMoreParam();
